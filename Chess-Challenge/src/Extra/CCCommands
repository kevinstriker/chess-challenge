# MyBot vs NegaMax - Tier 1
/Users/kevinvermaat/Documents/other/CuteChess1.3.0/CuteChess/build/cutechess-cli \
-engine name="MyBot" cmd="/Users/kevinvermaat/Documents/other/chess-challenge-kevin/Chess-Challenge/bin/Debug/net6.0/Chess-Challenge" arg="uci" arg="MyBot" \
-engine name="NegaMaxBot" cmd="/Users/kevinvermaat/Documents/other/chess-challenge-kevin/Chess-Challenge/bin/Debug/net6.0/Chess-Challenge" arg="uci" arg="NegaMaxBot" \
-each proto=uci tc=60+0 restart=on \
-sprt elo0=0 elo1=10 alpha=0.05 beta=0.05 \
-concurrency 10 -maxmoves 200 -rounds 20
-ratinginterval 20 \
-pgnout sprt.pgn

# MyBot vs JW Example - Tier 2
/Users/kevinvermaat/Documents/other/CuteChess1.3.0/CuteChess/build/cutechess-cli \
-engine name="MyBot" cmd="/Users/kevinvermaat/Documents/other/chess-challenge-kevin/Chess-Challenge/bin/Debug/net6.0/Chess-Challenge" arg="uci" arg="MyBot" \
-engine name="JacquesBot" cmd="/Users/kevinvermaat/Documents/other/chess-challenge-kevin/Chess-Challenge/bin/Debug/net6.0/Chess-Challenge" arg="uci" arg="JacquesBot" \
-each proto=uci tc=60+0 restart=on \
-sprt elo0=0 elo1=10 alpha=0.05 beta=0.05 \
-concurrency 5 -maxmoves 200 -rounds 20
-ratinginterval 20 \
-pgnout sprt.pgn

